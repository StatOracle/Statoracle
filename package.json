{
  "name": "statoracle-v2",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix": "pnpm run lint --fix",
    "pfix": "pnpm run prettier:fix",
    "prettier:fix": "prettier --write .",
    "db": "pnpm exec drizzle-kit push",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:fixtures": "stripe fixtures fixtures/stripe-fixtures.json",
    "supabase:start": "pnpm exec supabase start",
    "supabase:stop": "pnpm exec supabase stop",
    "supabase:status": "pnpm exec supabase status",
    "supabase:restart": "pnpm run supabase:stop && pnpm run supabase:start",
    "supabase:reset": "pnpm exec supabase db reset",
    "supabase:link": "pnpm exec supabase link",
    "supabase:generate-types": "pnpm exec supabase gen types typescript --local --schema public > types_db.ts",
    "supabase:generate-migration": "pnpm exec supabase db diff | pnpm exec supabase migration new",
    "supabase:generate-seed": "pnpm exec supabase db dump --data-only -f supabase/seed.sql",
    "supabase:push": "pnpm exec supabase db push",
    "supabase:pull": "pnpm exec supabase db pull",
    "email": "pnpm run email:dev",
    "email:build": "email build --dir src/emails",
    "email:dev": "email dev --dir src/emails",
    "email:export": "email export --dir src/emails"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@react-email/components": "^0.0.34",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@svgr/webpack": "^8.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^12.5.0",
    "lenis": "^1.2.3",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.5",
    "posthog-js": "^1.232.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "resend": "^4.1.2",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.17.25",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-email": "3.0.7",
    "tailwindcss": "^4.0.15",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
