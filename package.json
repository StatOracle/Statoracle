{
  "name": "statoracle-v2",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix": "pnpm run lint --fix",
    "pfix": "pnpm run prettier:fix",
    "prettier:fix": "prettier --write .",
    "db": "pnpm exec drizzle-kit push",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:fixtures": "stripe fixtures fixtures/stripe-fixtures.json",
    "supabase:start": "pnpm exec supabase start",
    "supabase:stop": "pnpm exec supabase stop",
    "supabase:status": "pnpm exec supabase status",
    "supabase:restart": "pnpm run supabase:stop && pnpm run supabase:start",
    "supabase:reset": "pnpm exec supabase db reset",
    "supabase:link": "pnpm exec supabase link",
    "supabase:generate-types": "pnpm exec supabase gen types typescript --local --schema public > types_db.ts",
    "supabase:generate-migration": "pnpm exec supabase db diff | pnpm exec supabase migration new",
    "supabase:generate-seed": "pnpm exec supabase db dump --data-only -f supabase/seed.sql",
    "supabase:push": "pnpm exec supabase db push",
    "supabase:pull": "pnpm exec supabase db pull",
    "email": "pnpm run email:dev",
    "email:build": "email build --dir src/emails",
    "email:dev": "email dev --dir src/emails",
    "email:export": "email export --dir src/emails",
    "payload:migrate-waitlist": "tsx src/scripts/migrate-waitlist-to-payload.ts",
    "payload:seed-templates": "tsx src/scripts/seed-email-templates.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@payloadcms/db-postgres": "^3.35.1",
    "@payloadcms/email-resend": "^3.35.1",
    "@payloadcms/next": "^3.35.1",
    "@payloadcms/next-payload": "^0.1.11",
    "@payloadcms/richtext-lexical": "^3.35.1",
    "@payloadcms/richtext-slate": "^3.35.1",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-toast": "^1.2.11",
    "@react-email/components": "^0.0.34",
    "@sentry/nextjs": "^9.14.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^12.9.2",
    "graphql": "^16.11.0",
    "lenis": "^1.3.1",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "next-themes": "^0.4.6",
    "payload": "^3.35.1",
    "postgres": "^3.4.5",
    "posthog-js": "^1.236.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "resend": "^4.4.1",
    "sharp": "^0.34.1",
    "sonner": "^2.0.3",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20.17.31",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.25.1",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-email": "3.0.7",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
