{
  "name": "statoracle-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:fixtures": "stripe fixtures fixtures/stripe-fixtures.json",
    "supabase:start": "pnpx supabase start",
    "supabase:stop": "pnpx supabase stop",
    "supabase:status": "pnpx supabase status",
    "supabase:restart": "pnpm run supabase:stop && pnpm run supabase:start",
    "supabase:reset": "pnpx supabase db reset",
    "supabase:link": "pnpx supabase link",
    "supabase:generate-types": "pnpx supabase gen types typescript --local --schema public > types_db.ts",
    "supabase:generate-migration": "pnpx supabase db diff | pnpx supabase migration new",
    "supabase:generate-seed": "pnpx supabase db dump --data-only -f supabase/seed.sql",
    "supabase:push": "pnpx supabase db push",
    "supabase:pull": "pnpx supabase db pull",
    "payload": "payload"
  },
  "dependencies": {
    "@radix-ui/react-toast": "^1.2.6",
    "@supabase/supabase-js": "^2.49.1",
    "@svgr/webpack": "^8.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.7",
    "lenis": "^1.1.21",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "payload": "^3.31.0",
    "posthog-js": "^1.223.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3"
  }
